<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import java.lang.Double?>
<AnchorPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="hilichurl.mondstadtadvanture.StartInterfaceController"
            stylesheets="@StartInterface.css"
            styleClass="root">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <VBox alignment="CENTER" spacing="20.0"
        AnchorPane.leftAnchor="20" AnchorPane.rightAnchor="20.0"
        AnchorPane.bottomAnchor="50.0">
        <!--  开始按钮  -->
        <Button text="开始游戏" onAction="#onStart"/>

        <!--  继续按钮  -->
        <Button text="继续游戏" onAction="#onContinous"/>

        <!--  设置按钮  -->
        <Button text="设置" onAction="#onSetting"/>

        <!--  退出按钮  -->
        <Button text="退出" onAction="#onExit"/>
    </VBox>
</AnchorPane>
